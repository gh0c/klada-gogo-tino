<% provide(:title, "Edit prediction") %>
<h1>Update prediction</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@prediction, html: { multipart: true }) do |f| %>
      <%= render 'fields', f: f %>
      <%= f.submit "Save changes", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>