<% provide(:title, 'Add player') %>
<h1>Add new player</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@player, html: { multipart: true }) do |f| %>
      <%= render 'fields', f: f %>
      <%= f.submit "Create player", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>